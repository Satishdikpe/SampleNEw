package Stringandswitch;

public class Test1 {
public static void main(String[] args) {
	int a = 1;
	
	switch(a)
	{
	case 0 :
		System.out.println("its case 0");
		
	case 1:
		System.out.println("its case one");
		
	case 2 :
		System.out.println("its case two");
		break;
		default:
			System.out.println("its default case ");
			break;
				
	}
		
}
}
